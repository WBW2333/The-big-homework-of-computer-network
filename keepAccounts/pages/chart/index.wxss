.selectBox{
    width: 400px;
}

#select_box{
    max-height: 240px;
    width: 398px;
}

.selectBox_list{
    max-height: 240px;
    width: 398px;
}

.progress-bar {
    width: 700rpx;
    height: 20px;
    text-align: center;
    line-height: 10px;
    box-sizing: border-box;
    /*margin-top: 50px;*/
    /*margin-bottom: 20px;*/
    /*border: 1px saddlebrown;*/
    border-style: solid;
    border-width: 1px;
    border-radius: 10px 10px;
    background: #FFF;
    position: relative;
    margin-left: 5px;
}
.progress {
    height: 16px;
    opacity: 1;
    background: rgb(260, 200, 60);
    color: RGBA(260, 200, 60, 0);
    overflow: hidden;
    position: absolute;
    text-align: center;
    border-style: solid;
    border-width: 1px;
    border-radius: 8px;
}

.chart {
    width: 750rpx;
    height: 600rpx;
    position: center;
    justify-content: center;
    align-items: center;
}

.wrap {
    width: 100%;
    height: 300px;
}

.selectChart_listLi {
    border: #000000;
    border-width: 1px;
}